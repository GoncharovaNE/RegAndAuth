<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:vm="using:RegAndAuth.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RegAndAuth.RegPage"
			 xmlns:conv="using:RegAndAuth.Converters"
			 x:DataType="vm:RegPageVM">

	<UserControl.Resources>
		<conv:MyConvertImage x:Key="MyConvertImage"></conv:MyConvertImage>
	</UserControl.Resources>

	<StackPanel>
		<Button Command="{Binding AddOnePhoto}">Добавить фото</Button>
		<TextBox Watermark="Введите ФИО" Text="{Binding NewUser.Fio}"></TextBox>
		<TextBox Watermark="Введите логин" Text="{Binding NewUser.Login}"></TextBox>
		<TextBox Watermark="Введите пароль" Text="{Binding NewPassword}"></TextBox>
		<Button Command="{Binding Adduser}" Content="{Binding StrBTReg}"></Button>
	</StackPanel>
</UserControl>