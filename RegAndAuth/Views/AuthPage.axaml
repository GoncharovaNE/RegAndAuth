<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:vm="using:RegAndAuth.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="RegAndAuth.AuthPage"
			 x:DataType="vm:AuthPageVM">
	<StackPanel>
		<Button Command="{Binding ToMainPage}">Отменить</Button>
		<TextBox Watermark="Введите логин" Text="{Binding Login}"></TextBox>
		<TextBox Watermark="Введите пароль" Text="{Binding Password}"></TextBox>
		<Button Command="{Binding AuthUser}">Авторизоваться</Button>
	</StackPanel>
</UserControl>
